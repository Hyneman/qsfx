all:

win32: qsfx.pb
	mkdir -p library
	mkdir -p library/x86
	pb32 qsfx.pb /EXE library/x86/qsfx32.dll /DLL /UNICODE /THREAD

win64: qsfx.pb
	mkdir -p library
	mkdir -p library/x64
	pb32 qsfx.pb /EXE library/x64/qsfx64.dll /DLL /UNICODE /THREAD

clean:
	rm -rf library

